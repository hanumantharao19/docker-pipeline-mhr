pipeline {
    agent any
    tools {
        maven  'maven'
    }
    stages {
        stage ('git checkout'){
            steps {
                cleanWs()
                checkout scm
                
            }
        }
        stage ('maven build'){
            steps {
                sh 'mvn'
            }
        }
    }
    post {
        always {
            echo " build is success completly always"
        }
        success {
            echo 'build is got sucess completly'
        }
        failure {
            echo "build is failed pelase cosole output for debug the logs
        }
      }
}
